{
  "index": "/index.html",
  "routing": {
    "routes": {
      "/": {
        "match": true
      },
      "/product": {
        "match": "prefix"
      },
      "/stock": {
        "match": "prefix"
      }
    }
  },
  "external": {
    "urls": [
      {
        "url": "https://fonts.googleapis.com/css?family=Chivo:400,700,900&display=swap",
        "url": "https://fonts.googleapis.com/css?family=Lato:100,300,400,700&display=swap"
      }
    ]
  },
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": ["/favicon.ico", "/index.html", "/*.css", "/*.js"]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    }
  ]
}
